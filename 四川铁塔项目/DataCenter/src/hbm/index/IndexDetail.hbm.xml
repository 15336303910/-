<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.function.index.model.IndexDetail" table="INDEX_DETAIL">
        <id name="ID" type="java.lang.Integer">
            <column name="ID" />
            <generator class="increment"/>
        </id>
        <property name="INDEX_NAME" type="java.lang.String">
            <column name="INDEX_NAME" length="100"/>
        </property>        
        <property name="CREATE_DATE" type="java.util.Date">
            <column name="CREATE_DATE"/>
        </property>
        <property name="IS_USING" type="java.lang.String">
            <column name="IS_USING" length="5"/>
        </property>
        <property name="INDEX_LEVEL" type="java.lang.Integer">
            <column name="INDEX_LEVEL"/>
        </property>
    </class>
</hibernate-mapping>