<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.function.rules.model.RuleJobItem" table="RULE_JOB_ITEM">
        <id name="ID" type="java.lang.Integer">
            <column name="ID" />
            <generator class="increment" />
        </id>
        <property name="JOB_ID" type="java.lang.Integer">
        	<column name="JOB_ID"/>
        </property>
        <property name="CHECK_COLUMN" type="java.lang.String">
            <column name="CHECK_COLUMN" length="100"/>
        </property>
        <property name="CHECK_EXPRESS" type="java.lang.String">
            <column name="CHECK_EXPRESS" length="100"/>
        </property>
        <property name="PROBLEM_COUNT" type="java.lang.Integer">
        	<column name="PROBLEM_COUNT"/>
        </property>
    </class>
</hibernate-mapping>