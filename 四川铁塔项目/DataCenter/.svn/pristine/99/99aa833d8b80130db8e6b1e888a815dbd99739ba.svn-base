html {
    overflow: hidden;
}
.navbar-static-top .container, 
.navbar-fixed-top .container, 
.navbar-fixed-bottom .container {
    padding: 0 15px;
    width: auto;
}
.navbar-fixed-top .navbar-inner, .navbar-static-top .navbar-inner {
    background: #3498DB;
    border:none;
    box-shadow:none;
    filter:none;
}
* + html .navbar-fixed-top .navbar-inner {
    height: 58px;
}
.navbar-fixed-bottom .navbar-inner {
    background: #3498DB;
    box-shadow: none;
    border: none;
}
.navbar .brand {
    width: 327px;
    height: 38px;
    margin: 16px 0;
    padding: 0;
}
.navbar .nav > li {
    margin: 5px;
}
.navbar .nav > li > a,
.navbar .nav > li > a:focus,
.navbar .nav > .active > a:focus {
    background: transparent;
    box-shadow: none;
    color: #ffffff;
    text-shadow: none;
    font-weight: normal;
    padding: 0;
    width: 80px;
    height:60px;
    border: none;
    padding: 4px 0;
}
.navbar .nav > .active > a,
.navbar .nav > li > a:hover,
.navbar .nav > .active > a:hover {
    background-color: transparent;
    box-shadow: none;
    border: none;
    background-image: url(../img/menuactive.png);
    width: 80px;
    height:60px;
    padding: 4px 0;
}
.nav > li > a > img {
    width: 30px;
    height: 30px;
    margin: 0px auto;
    display: block;
}
.navbar-inner-second {
    height: 36px;
    width: 100%;
    background-image: url(../img/second-nav.png);
    background-color: #3498DB;
}
.navsecond > li {
    float: left;
    list-style-type: none;
    height: 24px;
    line-height: 24px;
    margin: 6px 0;
    padding-left: 4px;
    position: relative;
}
.navsecond > li.active {
    background-image: url(../img/BarBg-a.png);
    background-repeat: no-repeat;
}
.navsecond > li.active > a {
    background-image: url(../img/BarBg-b.png);
    background-repeat: repeat-x;
}
.navsecond > li.active > .dropdown > a {
    background-image: url(../img/BarBg-b.png);
    background-repeat: repeat-x;
}
.navsecond > li > a {
    height: 24px;
    width: auto;
    color: #ffffff;
    padding: 0 15px;
    display: block;
    line-height: 24px;
}
.navsecond > li > .dropdown > a{
    height: 24px;
    width: auto;
    color: #ffffff;
    padding: 0 15px;
    display: block;
    line-height: 24px;
}
.navsecond > li > .dropdown .caret {
    margin-top: 10px;
    margin-left: 2px;
    border-top-color: #fff
}
.navsecond > li > .dropdown > .dropdown-menu{
    left: -5px;
    border: 1px solid #3498DB;
}
.navsecond > li.active > span.lastspan {
    background-image: url(../img/BarBg-c.png);
    background-repeat: no-repeat;
    position: absolute;
    right: -4px;
    top: 0;
    width: 4px;
    display: block;
    height: 24px;
    margin: 0;
}
.nav-text { 
    text-align:center;
    display: block;
    color: #ffffff;
    line-height: 18px;
    margin-top: 3px;
    height: 18px;
}
.footer-text {
    color: #ffffff;
    height: 20px;
    line-height: 20px;
    margin: 5px 0;
}
.footer-text a{
    color: #ffffff;
}

.content {
    margin: 106px 0 30px;
    overflow: hidden;
    position: relative;
}









