/*css reset*/
body,
div,
li,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
form,
fieldset,
input,
textarea,
p,
blockquote,
th,
td {
    margin: 0;
    padding: 0;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}

ol,
ul {
    list-style: none;
}

fieldset,
img {
    border: 0
}

a {
    text-decoration: none;
}

a:hover,
a:focus {
    text-decoration: none;
}

blockquote,
q {
    quotes: none;
}

blockquote:before,
blockquote:after,
q:before,
q:after {
    content: '';
    content: none;
}

abbr,
acronym {
    border: 0;
    font-variant: normal;
}

address,
cite,
dfn,
optgroup,
em,
var {
    font-style: normal;
}

legend {
    color: #000;
}

:focus {
    outline: 0;
}

caption,
th {
    text-align: left;
}


.div-open {
    overflow: hidden;
    _height: 1%;
    _overflow: visible;
}
