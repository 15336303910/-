body {
    color: #3e4855;
    font-size: 12px;
    font-family: 'Microsoft YaHei';
    min-width: 1024px;
    overflow: hidden;
}
/*css reset*/

body,
div,
li,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
form,
fieldset,
input,
textarea,
p,
blockquote,
th,
td {
    margin: 0;
    padding: 0;
}
table {
    border-collapse: collapse;
    border-spacing: 0;
}
ol,
ul {
    list-style: none;
}
fieldset,
img {
    border: 0
}
a {
    text-decoration: none;
}
a:hover {
    text-decoration: underline;
}
blockquote,
q {
    quotes: none;
}
blockquote:before,
blockquote:after,
q:before,
q:after {
    content: '';
    content: none;
}
abbr,
acronym {
    border: 0;
    font-variant: normal;
}
address,
cite,
dfn,
optgroup,
em,
var {
    font-style: normal;
}
legend {
    color: #000;
}
:focus {
    outline: 0;
}
caption,
th {
    text-align: left;
}
