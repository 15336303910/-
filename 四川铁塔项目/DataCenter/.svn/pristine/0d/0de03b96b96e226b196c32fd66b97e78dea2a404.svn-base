/* 鐧诲綍椤甸潰 */

.L-main {
    margin-left: 520px;
}
.L-logo {
    width: 520px;
    margin-left: -100%;
    background: url(../img/loginsider.jpg) left bottom no-repeat;
    position: relative;
}
.loginlogo {
    text-align: right;
    padding: 215px 47px 0 0;
    margin: 0
}
.loginfoot {
    padding-right: 47px;
    text-align: right;
    color: #7b87ba;
    position: absolute;
    right: 0;
    bottom: 40px;
}
.loginbox {
    padding: 215px 0 0 115px;
}
.loginbg {
    background-image: url(../img/body_bg.png);
}
.loginboxtitle {
    background: url(../img/loginboxtitle.png) left top no-repeat;
    height: 166px;
}
.loginformline {
    height: 44px;
}
.login-id {
    display: inline-block;
    width: 49px;
    height: 39px;
    background: url(../img/id.png) no-repeat;
    position: absolute;
    left: 1px;
    top: 1px;
}
.loginformline input {
    height: 42px;
    border: 1px solid #e5e5e7;
    width: 257px;
    border-radius: 2px;
    padding-left: 55px;
    font-size: 14px;
    font-weight: bolder;
    box-shadow: none;
    padding-bottom: 0;
    padding-top: 0;
    margin-bottom: 0
}
.login-pw {
    display: inline-block;
    width: 49px;
    height: 42px;
    background: url(../img/pw.png) no-repeat;
    position: absolute;
    left: 1px;
    top: 1px;
}
.loginline {
    width: 323px;
    overflow: hidden;
    padding: 0 1px;
}
.mt0 {
    margin-top: 0 !important
}
.loginbtn {
    display: inline-block;
    width: 320px;
    height: 56px;
    background: url(../img/loginbtn.png) no-repeat;
}
.loginbtn:hover {
    background: url(../img/loginbtnhover.png) no-repeat;
}
input:not([type='image']),
textarea{
	box-sizing:content-box;
}
