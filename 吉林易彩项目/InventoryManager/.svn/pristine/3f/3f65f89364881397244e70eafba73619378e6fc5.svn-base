package com.inspur.eoms.api;

import java.util.Properties;

public interface IConfiguration {

	/**
	 * 取得指定key的全局配置信息。
	 * 
	 * @param key
	 * @return
	 */
	public abstract String getConfig(String key);

	/**
	 * 取得全局配置集合.
	 * 
	 * @return
	 */
	public abstract Properties getConfigs();

	public abstract void setConfig(Properties config);

}